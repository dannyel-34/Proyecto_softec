<tr id=<%= "municipio_#{municipio.id}" %>>
   <td><%= municipio.nombre %></td>
   <td><%= link_to 'Mostrar', municipio, remote: true, class: "btn btn-primary" %></td>
   <td><%= link_to 'Editar', edit_municipio_path(municipio), remote: true, class: "btn btn-warning" %></td>
   <td><%= link_to 'Eliminar', municipio, method: :delete, remote: true, data: { confirm: 'Esta Seguro?' }, class: "btn btn-danger" %></td>
</tr>   