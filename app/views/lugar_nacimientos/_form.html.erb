<%= simple_form_for(@lugar_nacimiento) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre_departamento %>
    <%= f.input :nombre_municipio %>
    <%= f.input :nombre_centro_poblado %>
    <%= f.input :nombre %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
