<p id="notice"><%= notice %></p>

<h1>Listing Lugar Nacimientos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre departamento</th>
      <th>Nombre municipio</th>
      <th>Nombre centro poblado</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lugar_nacimientos.each do |lugar_nacimiento| %>
      <tr>
        <td><%= lugar_nacimiento.nombre_departamento %></td>
        <td><%= lugar_nacimiento.nombre_municipio %></td>
        <td><%= lugar_nacimiento.nombre_centro_poblado %></td>
        <td><%= lugar_nacimiento.nombre %></td>
        <td><%= link_to 'Show', lugar_nacimiento %></td>
        <td><%= link_to 'Edit', edit_lugar_nacimiento_path(lugar_nacimiento) %></td>
        <td><%= link_to 'Destroy', lugar_nacimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lugar nacimiento', new_lugar_nacimiento_path %>
