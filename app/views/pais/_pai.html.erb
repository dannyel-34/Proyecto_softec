
<tr id=<%= "pai_#{pai.id}" %>>
   <td><%= pai.nombre %></td>
   <td><%= link_to 'Mostrar', pai, remote: true, class: "btn btn-primary" %></td>
   <td><%= link_to 'Editar', edit_pai_path(pai), remote: true, class: "btn btn-warning" %></td>
   <td><%= link_to 'Eliminar', pai, method: :delete, remote: true, data: { confirm: 'Esta Seguro?' }, class: "btn btn-danger" %></td>
</tr>     
